
#include <stdio.h>
int num_sum(int arr[3][3], int r, int c, int k)
{
	int x = 0;
	int y = c - 1;
	while (x <= r - 1 && y >= 0)
	{
		if (k < arr[x][y])
		{
			y--;
		}
		else if (k > arr[x][y])
		{
			x++;
		}
		else
		{
			return 1;
		}

	}
	return 0;
}
int main()
{
	int arr[3][3] = { 0 };
	int k = 0;
	scanf_s("%d", &k);
	int ret = num_sum(arr, 3, 3, k);
	printf("%d\n", ret);
	return 0;
}
